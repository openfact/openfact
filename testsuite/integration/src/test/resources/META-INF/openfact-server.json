{
  "admin": {
    "organization": "master"
  },

  "adminConsole": {
    "url": "http://localhost:4200"
  },

  "eventsStore": {
    "provider": "${openfact.eventsStore.provider:jpa}"
  },

  "eventsListener": {
    "jboss-logging": {
      "success-level": "debug",
      "error-level": "warn"
    }
  },

  "organization": {
    "provider": "${openfact.organization.provider:jpa}"
  },

  "document": {
    "provider": "${openfact.invoice.provider:jpa}"
  },

  "invoice": {
    "provider": "${openfact.invoice.provider:jpa}"
  },

  "creditNote": {
    "provider": "${openfact.creditNote.provider:jpa}"
  },

  "debitNote": {
    "provider": "${openfact.debitNote.provider:jpa}"
  },

  "storageFile": {
    "provider": "${openfact.storageFile.provider:jpa}"
  },

  "file": {
    "provider": "${openfact.storageFile.provider:jpa}",
    "jpa": {},
    "dropbox": {
      "accessToken": "hhPoLjoclSAAAAAAAAAFaeE8cU53NgEmV3Sw0zKWZbPIEmvETOdai2gggvHkZqi6"
    }
  },

  "timer": {
    "provider": "basic"
  },

  "theme": {
    "staticMaxAge": "${openfact.theme.staticMaxAge:2592000}",
    "cacheTemplates": "${openfact.theme.cacheTemplates:true}",
    "cacheThemes": "${openfact.theme.cacheThemes:true}",
    "folder": {
      "dir": "${openfact.theme.dir}"
    }
  },

  "report": {
    "folder": {
      "dir": "${openfact.report.dir}"
    }
  },

  "scheduled": {
    "interval": 900
  },

  "connectionsHttpClient": {
    "default": {}
  },


  "connectionsJpa": {
    "default": {
      "url": "${openfact.connectionsJpa.url:jdbc:h2:mem:test}",
      "driver": "${openfact.connectionsJpa.driver:org.h2.Driver}",
      "driverDialect": "${openfact.connectionsJpa.driverDialect:}",
      "user": "${openfact.connectionsJpa.user:sa}",
      "password": "${openfact.connectionsJpa.password:}",
      "initializeEmpty": true,
      "migrationStrategy": "update",
      "showSql": "${openfact.connectionsJpa.showSql:false}",
      "formatSql": "${openfact.connectionsJpa.formatSql:true}",
      "globalStatsInterval": "${openfact.connectionsJpa.globalStatsInterval:-1}"
    }
  },

  "jta-lookup": {
    "provider": "${openfact.jta.lookup.provider:jboss}",
    "jboss": {
      "enabled": true
    }
  },

  "organizationScheduleTask": {
    "sendToThirdParty": {
      "active": true
    },
    "sendToCustomer": {
      "active": true
    }
  }
}