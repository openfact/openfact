{
  "admin": {
    "organization": "master"
  },
  "adminConsole": {
    "url": "http://localhost:4200"
  },
  "eventsListener": {
    "jboss-logging": {
      "success-level": "debug",
      "error-level": "warn"
    }
  },
  "theme": {
    "staticMaxAge": "${openfact.theme.staticMaxAge:2592000}",
    "cacheTemplates": "${openfact.theme.cacheTemplates:true}",
    "cacheThemes": "${openfact.theme.cacheThemes:true}",
    "folder": {
      "dir": "${openfact.theme.dir}"
    }
  },
  "report": {
    "folder": {
      "dir": "${openfact.report.dir}"
    }
  },
  "restSecurity": {
    "provider": "keycloak"
  },
  "ublSigner": {
    "provider": "default"
  },
  "invoice": {
    "readerWriter": "default",
    "idGenerator": "default",
    "modelCustomization": "default",
    "thirdPartySender": "default"
  },
  "credit_note": {
    "readerWriter": "default",
    "idGenerator": "default",
    "modelCustomization": "default",
    "thirdPartySender": "default"
  },
  "debit_note": {
    "readerWriter": "default",
    "idGenerator": "default",
    "modelCustomization": "default",
    "thirdPartySender": "default"
  }
}